<%= semantic_form_for [@system, @nav_entry] do |f| %>
  <% f.inputs  do %>
    <%= f.input :system_id, :as => :hidden %>
    <%= f.input :system, :input_html => { :disabled => true } %>
    <%= f.input :parent, :collection => @parents %>
    <%= f.input :seq %>
    <%= f.input :content %>
    <%= f.input :description %>
  <% end%>
  <%= f.buttons %>
<% end %>
